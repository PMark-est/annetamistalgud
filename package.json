{
  "name": "annetamistalgud",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "npm-run-all --parallel dev:api dev:vite",
    "dev:api": "nodemon --watch server --ext ts,tsx --exec tsx server/main.ts",
    "dev:vite": "vite",
    "build": "vite build && npm run bundle --prefix ./server",
    "copy:prisma": "npx prisma generate && cp -r prisma dist/prisma && cp -r generated dist/generated && cp -r node_modules/.prisma/client dist/generated/prisma",
    "lint": "eslint .",
    "preview": "vite preview"
  },
  "prisma": {
    "schema": "./prisma"
  },
  "dependencies": {
    "@prisma/client": "^6.13.0",
    "@tailwindcss/vite": "^4.1.11",
    "axios": "^1.11.0",
    "esbuild": "^0.25.8",
    "esbuild-plugin-alias": "^0.2.1",
    "express": "^5.1.0",
    "jsonwebtoken": "^9.0.2",
    "prisma": "^6.13.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-router-dom": "^7.7.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.30.1",
    "@types/express": "^5.0.3",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@types/react-router-dom": "^5.3.3",
    "@vitejs/plugin-react": "^4.6.0",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.30.1",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "nodemon": "^3.1.10",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "tailwindcss": "^4.1.11",
    "ts-node-dev": "^2.0.0",
    "tsx": "^4.20.3",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.35.1",
    "vite": "^7.0.4"
  }
}
